<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>BuildTaskVersionControl</id>
    <version>1.0.0</version>
    <title>Version Control</title>
    <authors>Truinto</authors>
    <developmentDependency>true</developmentDependency>
    <license type="expression">MIT</license>
    <licenseUrl>https://licenses.nuget.org/MIT</licenseUrl>
    <description>MSBuild Task to automate assembly versioning</description>
    <repository url="https://github.com/Truinto/" />
    <dependencies>
      <group targetFramework=".NETFramework4.7.2">
        <dependency id="Microsoft.Build.Framework" version="17.3.1" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Build.Utilities.Core" version="17.3.1" exclude="Build,Analyzers" />
      </group>
      <group targetFramework="net6.0">
        <dependency id="Microsoft.Build.Framework" version="17.3.1" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Build.Utilities.Core" version="17.3.1" exclude="Build,Analyzers" />
      </group>
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Net.Http" targetFramework=".NETFramework4.7.2" />
    </frameworkAssemblies>
  </metadata>
</package>